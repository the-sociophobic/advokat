{{rentals-search rentals=model}}
<div class="container">  
  <div class="text-center">Найдено {{model.length}} {{numerals-counter model.length 'предложение' 'предложения' 'предложений'}}</div>
  <ul class="results">
    {{#each model as |rentalUnit|}}
      <li>{{rental-listing rental=rentalUnit}}</li>
    {{/each}}
  </ul>
</div>